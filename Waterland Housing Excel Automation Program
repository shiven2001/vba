Private Sub UserForm_Terminate()
If Not (wb Is Nothing) Then
wb.Windows(1).Visible = True
End If

If Not (wbUU Is Nothing) Then
wbUU.Windows(1).Visible = True
End If

If Not (wbcon Is Nothing) Then
wbcon.Windows(1).Visible = True
End If



End Sub


